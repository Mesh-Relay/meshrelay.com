{
  "name": "meshrelay.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@honeybadger-io/react": "0.0.5",
    "@storybook/react": "^5.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "honeybadger-js": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy-production": "react-scripts build && scp -r ./build/* ubuntu@167.71.107.103:/var/www/meshrelay.com/html",
    "cy:open": "cypress open",
    "cy:dev": "cypress run --project ./cypress --record false",
    "cy:run": "cypress run --project ./cypress --record",
    "test": "react-scripts test",
    "test:ci": "start-server-and-test start 3000 cy:run",
    "prettier": "prettier --write **/*.{ts,js,css,html}",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-links": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "cypress": "^3.8.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,js,css,json,html,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
